<?xml version="1.0" encoding="utf-8"?>
<r id="rusmyv" xml:lang="rus">
<e>
   <lg>
     <l pos="Pr">в</l>
   </lg>
   <mg><defNative>с винит. падежей (при обозначении направления)</defNative>
     <tg xml:lang="myv">
       <t>-c</t>
     <xg><x>в дом</x>
       <xt xml:lang="myv">кудос</xt>
     </xg>
     <xg><x>в лес</x>
       <xt xml:lang="myv">вирьс</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с предл. п. (при обозначении местонахождения)</defNative>
     <tg xml:lang="myv">
       <t>-со/-сэ</t>
     <xg><x>в лесу</x>
       <xt xml:lang="myv">вирьсэ</xt>
     </xg>
     <xg><x>в поле</x>
       <xt xml:lang="myv">паксясо</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (при обозначении времени года, месяца, дня)</defNative>
     <tg xml:lang="myv">
       <t>-сто/-стэ</t>
     <xg><x>мы приехали в тот день</x>
       <xt xml:lang="myv">минь сынек се чистэнть</xt>
     </xg>
     </tg>
   </mg>
</e>
<e>
   <lg>
     <l pos="Pr">за</l>
   </lg>
   <mg><defNative>с твор. п. (около)</defNative>
     <tg xml:lang="myv">
       <t>экшсэ</t>
     <xg><x>преподаватель сидит за столом</x>
       <xt xml:lang="myv">тонавтыцясь ашти столь экшсэ</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (около)</defNative>
     <tg xml:lang="myv">
       <t>экшс</t>
     <xg><x>преподаватель сел за стол</x>
       <xt xml:lang="myv">тонавтыцясь озась столь экшс</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с твор. п. (позади, по ту сторону)</defNative>
     <tg xml:lang="myv">
       <t>удало, томбале</t>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (позади, по ту сторону)</defNative>
     <tg xml:lang="myv">
       <t>удалов, томбалев</t>
     </tg>
   </mg>
   <mg><defNative>с твор. п. (вслед)</defNative>
     <tg xml:lang="myv">
       <t>мельга</t>
     <xg><x>один за другим</x>
       <xt xml:lang="myv">мельга мельцек</xt>
       <xt xml:lang="myv">вейке-вейке мельга</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с твор. п. (с целью)</defNative>
     <tg xml:lang="myv">
       <t>-с</t>
       <t>мельга</t>
     <xg><x>ходить за грибами</x>
       <xt xml:lang="myv">якамс пангс</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (на расстоянии: за Х от У)</defNative>
     <tg xml:lang="myv">
       <t>-до/-дэ/-де/-то/-тэ/-те</t>
     <xg><x>за два километра от Мокшалея</x>
       <xt xml:lang="myv">кавто вайгельпеде Мокшалеенть эйстэ</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (раньше: за Х до У)</defNative>
     <tg xml:lang="myv">
       <t>-ДО икеле</t>
     <xg><x>за два дня до отъезда</x>
       <xt xml:lang="myv">туемадонть кавто чиде икеле</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (в течение)</defNative>
     <tg xml:lang="myv">
       <t>-сто/-стэ</t>
     <xg><x>за последние три года</x>
       <xt xml:lang="myv">ютазь колмо иетнестэ</xt>
     </xg>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (взамен)</defNative>
     <tg xml:lang="myv">
       <t>таркас</t>
       <t>эземс</t>
     </tg>
   </mg>
   <mg><defNative>с винит. п. (с глаголами: достать, взять, голосовать)</defNative>
     <tg xml:lang="myv">
       <t>кисэ</t>
       <t>кис</t>
     <xg><x>голосовать за кого-нибудь кандидата</x>
       <xt xml:lang="myv">максомс вайгель кона-кона кандидатонть кис</xt>
     </xg>
     <xg><x>за подписью (кого-л.)</x>
       <xt xml:lang="myv">(кинь-бути) подписьсэ</xt>
     </xg>
     <xg><x>за неимением чего-л.</x>
       <xt xml:lang="myv">мезень-бути аразде</xt>
     </xg>
     <xg><x>за и против</x>
       <xt xml:lang="myv">кисэ ды каршо</xt>
     </xg>
     <xg><x>за счёт кого-л.</x>
       <xt xml:lang="myv">кинь-бути пельде</xt>
     </xg>
     <xg><x>за тридцать лет (о возрасте)</x>
       <xt xml:lang="myv">колоньгеменде ламо иесэ</xt>
     </xg>
     <xg><x>очередь за вами</x>очередесь тынк мельга
     </xg>
     <xg><x>за здоровье (кого-либо)</x>
       <xt xml:lang="myv">(кинь-бути) шумбрачинь кисэ</xt>
     </xg>
     </tg>
   </mg>
</e>

</r>
